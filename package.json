{
  "name": "stridejs",
  "version": "0.10.0-alpha",
  "description": "The JavaScript SDK for Stride",
  "contributors": [
    "Dan Lynch <pyramation@gmail.com>",
    "Stride Labs"
  ],
  "homepage": "https://github.com/Stride-Labs/stridejs#readme",
  "license": "MIT",
  "main": "main/index.js",
  "module": "module/index.js",
  "typings": "types/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "types",
    "main",
    "module"
  ],
  "scripts": {
    "build:main": "cross-env BABEL_ENV=production babel src --out-dir main --delete-dir-on-start --extensions \".tsx,.ts,.js\"",
    "build:module": "cross-env MODULE=true babel src --out-dir module --delete-dir-on-start  --extensions \".tsx,.ts,.js\"",
    "build": "npm run build:module && npm run build:main",
    "build:ts": "tsc --project ./tsconfig.json",
    "buidl": "npm run build && npm run build:ts",
    "codegen": "npm run codegen:cleanup && npm run codegen:clone && npm run codegen:build-stride && npm run codegen:build-cosmos-sdk && npm run codegen:build-ibc-go && npm run codegen:run",
    "codegen:cleanup": "cross-env rm -rf src/codegen scripts/proto scripts/repos",
    "codegen:clone": "cross-env NODE_ENV=development ts-node scripts/clone_repos.ts",
    "codegen:build-stride": "buf export scripts/repos/stride --output=scripts/proto",
    "codegen:build-cosmos-sdk": "buf export scripts/repos/cosmos-sdk --output=scripts/proto",
    "codegen:build-ibc-go": "buf export scripts/repos/ibc-go --output=scripts/proto",
    "codegen:run": "cross-env NODE_ENV=development ts-node scripts/codegen.ts",
    "prepare": "npm run build",
    "dev": "cross-env NODE_ENV=development babel-node src/index --extensions \".tsx,.ts,.js\"",
    "watch": "cross-env NODE_ENV=development babel-watch src/index --extensions \".tsx,.ts,.js\"",
    "lint": "eslint src --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Stride-Labs/stridejs"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Stride-Labs/stridejs/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/node": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.18.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.18.9",
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@babel/plugin-transform-runtime": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@bufbuild/buf": "^1.21.0-1",
    "@cosmology/telescope": "1.8.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-watch": "^7.0.0",
    "cross-env": "^7.0.2",
    "eslint": "8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "long": "^5.2.0",
    "prettier": "^2.7.1",
    "ts-node": "latest",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@cosmjs/amino": "0.31.3",
    "@cosmjs/encoding": "0.31.3",
    "@cosmjs/math": "0.31.3",
    "@cosmjs/proto-signing": "0.31.3",
    "@cosmjs/stargate": "0.31.3",
    "@cosmjs/tendermint-rpc": "0.31.3",
    "@cosmjs/utils": "0.31.3",
    "@cosmology/lcd": "^0.13.4",
    "@noble/hashes": "^1.4.0",
    "@osmonauts/helpers": "^0.6.0",
    "@osmonauts/lcd": "^0.6.0",
    "bech32": "^2.0.0",
    "cosmjs-types": "^0.9.0",
    "protobufjs": "^6.11.2"
  },
  "resolutions": {
    "long": "^5.2.0"
  }
}
